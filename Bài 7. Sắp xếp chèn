#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main(){
    int n;cin>>n;
    int a[n];
    for(int &x:a)cin>>x;
    for(int i=1;i<n;i++){
        cout<<"Buoc "<<i<<": ";
        sort(a,a+i+1);
        for(int i=0;i<n;i++)cout<<a[i]<<" ";
        cout<<endl;
    }
}
    
      
