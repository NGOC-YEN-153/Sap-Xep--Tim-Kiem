#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main(){
    int n;cin>>n;
    int a[n];
    for(int &x:a)cin>>x;
    for(int i=0;i<n-1;i++){
        cout<<"Buoc "<<i+1<<": ";
        for(int j=0;j<n-1;j++){
            if(a[j+1]<a[j])swap(a[j+1],a[j]);
        }
        for(int h=0;h<n;h++)cout<<a[h]<<" ";
        cout<<endl;
    }
}
    
      
